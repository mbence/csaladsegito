        <br>
    <!--    <button type="submit" name="import" value="all" onclick="return confirm('{{ 'Biztos benne?'|trans() }}')">Import All Tables</button> -->
    </fieldset>
    </form>
</div>

<div class="adminblock leftpad20">
    <form action="" method="POST" name="start">
        <fieldset>
        <legend>Gyermekjólét</legend>
        <dl class="import_controls">

            <dt>1) Truncate</dt>
            <dd>
                <button type="submit" class="button" name="import" value="truncate" onclick="return confirm('{{ 'Biztos benne?'|trans() }}')">Truncate tables &nbsp;</button>
            </dd>
            {% for sheet in sheets %}
            <dt>{{ loop.index + 1 }}) {{ sheet.name }}</dt>
            <dd>
                <button type="submit" class="button" name="import" id="import_{{ sheet.id }}" value="{{ sheet.id }}" {#onclick="return confirm('{{ 'Biztos benne?'|trans() }}')"#}>Import &nbsp;</button>
            </dd>
            {% endfor %}
        </dl>
        {% if results.gyejo is defined %}
            <div>{{ results.gyejo }}</div>
        {% endif %}
        </fieldset>
        {% if reload.from is defined %}
            <input type="hidden" name="from" value="{{ reload.from }}">
        {% endif %}
    </form>
</div>