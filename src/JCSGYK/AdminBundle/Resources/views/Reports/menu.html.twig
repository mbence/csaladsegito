<div class="menu">
    {% for item in menu %}
        <a href="{{ path('reports', {'report': item.slug}) }}"
           class="{% if item.class is defined %}{{ item.class}}{% endif %}"
           {% if item.id is defined %}id="{{ item.id }}"{% endif %}>
            {{ item.label|trans() }}
        </a>
    {% endfor %}
</div>