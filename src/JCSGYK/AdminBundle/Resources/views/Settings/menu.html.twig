<div class="menuitems">
    {% for item in menu %}
        <a href="{{ path(item.route) }}"
           class="menuitem {% if item.class is defined %}{{ item.class}}{% endif %}"
           {% if item.id is defined %}id="{{ item.id }}"{% endif %}>
            {{ item.label|trans() }}
        </a>
    {% endfor %}
</div>